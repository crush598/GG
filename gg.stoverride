


name: WPS Office
desc: 解锁超级会员部分功能

http:
  mitm:
    - "cubox.pro"
  script:
    - match: ^https:\/\/cubox\.pro\/c\/api\/userInfo
      name: wps
      type: response
      require-body: true
      timeout: 10
      
script-providers:
  wps:
    url: https://raw.githubusercontent.com/Yu9191/Rewrite/main/cubox.js
    interval: 86400
